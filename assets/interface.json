        "掃蕩次數": {
            "cases": [
                {
                    "name": "1次",
                    "pipeline_override": {
                        "TargetRaidTimes": {
                            "expected": "1"
                        }
                    }
                },
                {
                    "name": "2次",
                    "pipeline_override": {
                        "TargetRaidTimes": {
                            "expected": "2"
                        }
