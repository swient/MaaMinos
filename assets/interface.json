                        "EnterMaterialsItem": {
                            "expected": "詭秘"
                        },
                        "VerifyMaterialsItem": {
                            "expected": "詭秘"
                        }
                    }
                },
                {
                    "name": "精準源質",
                    "pipeline_override": {
                        "EnterMaterials": {
                            "expected": "帕爾"
                        },
                        "EnterMaterialsItem": {
                            "expected": "精準"
