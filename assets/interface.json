{
    "name": "MaaMinos",
    "url": "https://github.com/swient/MaaMinos",
    "controller": [
        {
            "name": "安卓端",
            "type": "Adb"
        }
    ],
    "resource": [
        {
            "name": "國際服",
            "path": [
                "{PROJECT_DIR}/resource/base"
            ]
        }
    ],
    "agent": {
        "child_exec": "python",
        "child_args": [
            "{PROJECT_DIR}/agent/main.py"
        ]
    },
    "task": [
        {
            "name": "啟動遊戲",
            "entry": "StartUp",
            "pipeline_override": {
                "ReturnHome": {
                    "next": []
                },
                "ResourcesObtained": {
                    "post_delay": 1500
                }
            }
        },
        {
            "name": "秘盟捐贈",
            "entry": "SocietyDonation",
            "doc": "默認捐贈狄斯幣",
            "pipeline_override": {
                "EnterNexus": {
                    "next": "EnterSociety"
                },
                "EnterSociety": {
                    "next": "EnterDonation"
                }
            }
        },
        {
            "name": "秘盟互助",
            "entry": "SocietyMutalAid",
            "doc": "默認求助技能模組I",
            "pipeline_override": {
                "EnterNexus": {
                    "next": "EnterSociety"
                },
                "EnterSociety": {
                    "next": "EnterMutalAid"
                }
            }
        },
        {
            "name": "收取監管",
            "entry": "System",
            "doc": "三星事件默認選擇第一項",
            "pipeline_override": {
                "EnterNexus": {
                    "next": "EnterSystem"
                }
            }
        },
        {
            "name": "派遣任務",
            "entry": "Dispatch",
            "pipeline_override": {
                "EnterOpsHub": {
                    "next": "EnterBureau"
                },
                "EnterBureau": {
                    "next": "EnterDispatch"
                }
            }
        },
        {
            "name": "濁暗之阱",
            "doc": "僅完成掃蕩工作,需完成樂園幻境",
            "entry": "OblivionPit",
            "pipeline_override": {
                "EnterOpsHub": {
                    "next": "EnterDiscity"
                },
                "EnterDiscity": {
                    "next": "EnterDisSea"
                },
                "EnterDisSea": {
                    "next": "EnterOblivionPit",
                    "interrupt": "AutoSwipeLeft"
                }
            }
        },
        {
            "name": "破碎防線",
            "entry": "BrokenFrontline",
            "doc": "僅領取本期與關卡獎勵",
            "pipeline_override": {
                "EnterOpsHub": {
                    "next": "EnterDiscity"
                },
                "EnterDiscity": {
                    "next": "EnterDisSea"
                },
                "EnterDisSea": {
                    "next": "EnterBrokenFrontline",
                    "interrupt": "AutoSwipeLeft"
                }
            }
        },
        {
            "name": "領取獎勵",
            "entry": "Awards",
            "option": [
                "領取友情點",
                "收取信箱",
                "領取體力"
            ],
            "pipeline_override": {
                "CompletedFriendship": {
                    "next": [
                        "MinosMailbox",
                        "Stamina",
                        "ReturnHome"
                    ]
                },
                "CompletedMinosMailbox": {
                    "next": [
                        "Stamina",
                        "ReturnHome"
                    ]
                }
            }
        },
        {
            "name": "材料掃蕩",
            "entry": "Materials",
            "doc": "不足目標掃蕩次數會掃蕩到可掃蕩的最大值",
            "option": [
                "掃蕩材料",
                "掃蕩次數",
                "使用全部體力"
            ],
            "pipeline_override": {
                "EnterOpsHub": {
                    "next": "EnterDiscity"
                },
                "EnterDiscity": {
                    "next": "EnterTheRust"
                },
                "EnterTheRust": {
                    "next": [
                        "EnterMaterials",
                        "EnterTheRust"
                    ]
                }
            }
        },
        {
            "name": "記憶風暴",
            "entry": "StormyMemories",
            "doc": "選擇三次以上掃蕩次數也只會掃蕩三次",
            "option": [
                "記憶畸點",
                "印象畸點",
                "殘響畸點",
                "掃蕩次數"
            ],
            "pipeline_override": {
                "EnterOpsHub": {
                    "next": "EnterDiscity"
                },
                "EnterDiscity": {
                    "next": "EnterTheRust"
                },
                "EnterTheRust": {
                    "next": [
                        "EnterStormyMemories",
                        "EnterTheRust"
                    ]
                },
                "SkipRaidLevel": {
                    "action": "DoNothing",
                    "next": []
                },
                "CompletedRaidLevel": {
                    "action": "DoNothing",
                    "next": []
                }
            }
        },
        {
            "name": "採購部",
            "entry": "SupplyOffice",
            "doc": "同名材料僅購買特惠材料",
            "option": [
                "每日免費補給",
                "搜索令(追蹤兌換)",
                "內海遺物箱(戰勳兌換)",
                "S級魂識印記(樣本兌換)",
                "A級魂識印記(樣本兌換)",
                "B級魂識印記(樣本兌換)",
                "低階材料補給箱(秘盟兌換)",
                "技能模組(秘盟兌換)",
                "狄斯幣(秘盟兌換)",
                "狂亂精粹(秘盟兌換)",
                "搜索令(友情兌換)",
                "夢影之塵(友情兌換)",
                "迴響質料(友情兌換)",
                "邊界重構碎片(友情兌換)",
                "一階源素補給箱(友情兌換)",
                "低階材料補給箱(友情兌換)",
                "技能模組(友情兌換)",
                "狄斯幣(友情兌換)",
                "狂亂精粹(友情兌換)",
                "記憶碎片(友情兌換)"
            ],
            "pipeline_override": {
                "EnterOpsHub": {
                    "next": "EnterSupplyOffice"
                }
            }
        },
        {
            "name": "監察密令",
            "entry": "Surveillance",
            "option": [
                "監察嘉獎"
            ],
            "pipeline_override": {
                "EnterOpsHub": {
                    "next": "EnterSurveillance"
                }
            }
        }
    ],
    "option": {
        "領取友情點": {
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {
                        "EnterNexus": {
                            "next": "EnterFriends"
                        }
                    }
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "Friendship": {
                            "enabled": false
                        }
                    }
                }
            ]
        },
        "收取信箱": {
            "cases": [
                {
                    "name": "Yes",
                    "pipeline_override": {}
                },
                {
                    "name": "No",
                    "pipeline_override": {
                        "MinosMailbox": {
                            "enabled": false
                        }
                    }
                }
            ]
